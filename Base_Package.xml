<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MudletPackage>
<MudletPackage version="1.0">
    <ScriptPackage>
        <Script isActive="no" isFolder="no">
            <name>Load Scripts</name>
            <packageName></packageName>
            <script>
              registerAnonymousEventHandler("sysLoadEvent","core.setup")
              local directory = getMudletHomeDir() .. "/dartmudlet/"
              local coreFile = directory.."/_core.lua"

              core = dofile(coreFile)
            </script>
            <eventHandlerList/>
        </Script>
    </ScriptPackage>
    <AliasPackage>
        <Alias isActive="yes" isFolder="no">
            <name>Unsetup</name>
            <script>core.unsetup()</script>
            <command></command>
            <packageName></packageName>
            <regex>/unsetup</regex>
        </Alias>
        <Alias isActive="yes" isFolder="no">
            <name>Setup</name>
            <script>core.setup()</script>
            <command></command>
            <packageName></packageName>
            <regex>/setup</regex>
        </Alias>
        <Alias isActive="yes" isFolder="no">
            <name>Resetup</name>
            <script>core.resetup()</script>
            <command></command>
            <packageName></packageName>
            <regex>/resetup</regex>
        </Alias>
    </AliasPackage>
</MudletPackage>
